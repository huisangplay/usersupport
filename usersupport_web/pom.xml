        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-data-jpa</artifactId>
            <version>RELEASE</version>
            <scope>compile</scope>
            <systemPath>${basedir}\src\lib\spring-data-jpa-2.5.4.jar</systemPath>
        </dependency>

        <dependency>
            <groupId>mysql</groupId>
            <artifactId>mysql-connector-java</artifactId>
            <version>8.0.26</version>
            <systemPath>${basedir}\src\lib\mysql-connector-java-8.0.26.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.12.0</version>
            <systemPath>${basedir}\src\lib\commons-lang3-3.12.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.comac</groupId>
            <artifactId>isc_sso_agent</artifactId>
            <version>2.1.14</version>
            <systemPath>${basedir}\src\lib\isc_sso_agent-2.1.14.jar</systemPath>
