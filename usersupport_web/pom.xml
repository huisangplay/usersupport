        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-configuration-processor</artifactId>
            <systemPath>${basedir}\src\lib\spring-boot-configuration-processor-2.5.4.jar</systemPath>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>org.apache.poi</groupId>
            <artifactId>poi-ooxml</artifactId>
            <version>5.0.0</version>
            <systemPath>${basedir}\src\lib\poi-ooxml-5.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>org.apache.poi</groupId>
            <artifactId>poi</artifactId>
            <version>5.0.0</version>
            <systemPath>${basedir}\src\lib\poi-5.0.0.jar</systemPath>
        </dependency>
